<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="havvg.jasmine.twig.extension.include_bundle_tests.class">Havvg\Bundle\JasmineBundle\Twig\Extension\IncludeBundleTestsExtension</parameter>
    </parameters>

    <services>
        <service id="havvg.jasmine.twig.extension.include_bundle_tests" class="%havvg.jasmine.twig.extension.include_bundle_tests.class%">
            <argument type="service" id="kernel" />
            <tag name="twig.extension" />
        </service>
    </services>
</container>